{
  "targets": {
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "@org/ui-next:build",
        "port": 3000
      }
    },

    "dev": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "@org/ui-next:build",
        "port": 3000
      }
    },

    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/ui-next"
      }
    }
  }
}
